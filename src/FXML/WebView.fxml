<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.web.WebView?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="WebViewController">
   <children>
      <WebView fx:id="webView" layoutY="85.0" prefHeight="316.0" prefWidth="600.0" />
      <Button layoutX="167.0" layoutY="52.0" mnemonicParsing="false" onAction="#loadPage" text="Load" />
      <TextField fx:id="textField" layoutX="5.0" layoutY="14.0" onAction="#loadPage" prefHeight="26.0" prefWidth="585.0" />
      <Button layoutX="14.0" layoutY="52.0" mnemonicParsing="false" onAction="#refreshPage" text="Reload" />
      <Button layoutX="121.0" layoutY="52.0" mnemonicParsing="false" onAction="#zoomIn" text="+" />
      <Button layoutX="146.0" layoutY="52.0" mnemonicParsing="false" onAction="#zoomOut" text="-" />
      <Button layoutX="67.0" layoutY="52.0" mnemonicParsing="false" onAction="#showHistory" text="History" />
      <Button layoutX="210.0" layoutY="52.0" mnemonicParsing="false" onAction="#back" text="Back" />
      <Button layoutX="251.0" layoutY="52.0" mnemonicParsing="false" onAction="#forward" text="Forward" />
      <Button layoutX="310.0" layoutY="52.0" mnemonicParsing="false" onAction="#executeJS" text="Execute JS" />
   </children>
</AnchorPane>
